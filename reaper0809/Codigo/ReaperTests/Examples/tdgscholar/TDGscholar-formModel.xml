<?xml version="1.0" ?>        
<formModel identificationUrl="http://scholar.tdg-seville.info">  
	<reachFormMethod>
		<httpGetRequest requestUrl="http://scholar.tdg-seville.info/Home.aspx"/>
	</reachFormMethod>

	<submitLocator>
		<locator-xpath value="//input[@title='Search']"/>
	</submitLocator>

	<fields>
		<textBox fieldId="query">
			<locator-id value="query"/>
		</textBox>
	</fields>
	
	<results>
		<result urlRegExp="Java">
			<action-classify tag="interestingJavaRelatedHubs" />
		</result>
		
		<result contentRegExp=".*">
			<action-iterateHub>
				<product>
					<locator-xpath value="//child::div[contains(@class,'publication')]//div[@class='title']//a"/>
				</product>
			</action-iterateHub>
		</result>
	</results>	
	
</formModel>
